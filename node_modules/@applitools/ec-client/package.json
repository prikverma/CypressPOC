{
  "name": "@applitools/ec-client",
  "version": "1.7.25",
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/ec-client"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./types/cli/cli.d.ts",
      "default": "./dist/cli/cli.js"
    },
    "./types": {
      "types": "./types/types.d.ts",
      "default": "./dist/types.js"
    },
    "./dist/*": {
      "types": "./types/*.d.ts",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "bin": {
    "ec-client": "./dist/cli/cli.js",
    "eg-client": "./dist/cli/cli.js"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "run --top-level tsc --project ./tsconfig.build.json",
    "test": "run --top-level mocha './test/**/*.spec.ts' --parallel --jobs ${MOCHA_JOBS:-15}",
    "test:e2e": "MOCHA_GROUP=e2e run --top-level mocha './test/e2e/*.spec.ts'",
    "test:it": "MOCHA_GROUP=it run --top-level mocha './test/it/*.spec.ts'"
  },
  "dependencies": {
    "@applitools/core-base": "1.9.0",
    "@applitools/driver": "1.16.2",
    "@applitools/logger": "2.0.14",
    "@applitools/req": "1.6.4",
    "@applitools/socket": "1.1.14",
    "@applitools/spec-driver-webdriver": "1.0.57",
    "@applitools/tunnel-client": "1.4.0",
    "@applitools/utils": "1.7.0",
    "abort-controller": "3.0.0",
    "webdriver": "7.31.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@applitools/test-server": "^1.2.2",
    "@applitools/test-utils": "^1.5.17",
    "@types/node": "^12.20.55",
    "@types/node-fetch": "^2.6.1",
    "@types/selenium-webdriver": "^4.0.19",
    "@types/yargs": "^17.0.19",
    "nock": "^13.3.2",
    "selenium-webdriver": "^4.15.0"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}